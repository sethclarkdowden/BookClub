<div class="span6 offset2">
  <div class="row-fluid">
    <h2>List of Users</h2>
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>#</th>
          <th>Title</th>
          <th>Acess token</th>
        </tr>
      </thead>

      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= link_to user.name, user %></td>
          <td><%= user.access_token %></td>
        </tr>
      <% end -%>
    </table>
  </div>
</div>
